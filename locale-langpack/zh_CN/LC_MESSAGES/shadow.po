msgid "\t%s: %s\n"
msgstr "\t%s：%s\n"

msgid "\n"
msgstr "\n"

msgid ""
"\n"
"%s login: "
msgstr ""
"\n"
"%s 用户名： "

msgid ""
"\n"
"Login timed out after %u seconds.\n"
msgstr ""
"\n"
"%u 秒后登录超时。\n"

msgid ""
"\n"
"System closed for routine maintenance"
msgstr ""
"\n"
"系统关闭，例行维护"

msgid ""
"\n"
"Type control-d to proceed with normal startup,\n"
"(or give root password for system maintenance):"
msgstr ""
"\n"
"敲击 control-d 继续使用普通帐户环境，\n"
"(或者输入 root 密码以进行系统维护)："

msgid ""
"\n"
"Warning: weak password (enter it again to use it anyway)."
msgstr ""
"\n"
"警告：脆弱的密码(重新输入以强制使用它)。"

msgid ""
"\n"
"[Disconnect bypassed -- root login allowed.]"
msgstr ""
"\n"
"[忽略断线要求 -- 允许 root 登录。]"

msgid "       %s [-p] -r host\n"
msgstr "       %s [-p] -r 主机\n"

msgid "       %s [-p] [-h host] [-f name]\n"
msgstr "       %s [-p] [-h 主机] [-f 名称]\n"

msgid ""
"  -A, --administrators ADMIN,...\n"
"                                set the list of administrators for GROUP\n"
msgstr "  -A, --administrators ADMIN,...\t设置组的管理员列表\n"

msgid ""
"  -D, --defaults                print or change default useradd "
"configuration\n"
msgstr "  -D, --defaults\t\t显示或更改默认的 useradd 配置\n"

msgid ""
"  -E, --expiredate EXPIRE_DATE  set account expiration date to EXPIRE_DATE\n"
msgstr "  -E, --expiredate 过期日期     将帐户过期时间设为“过期日期”\n"

msgid ""
"  -G, --groups GROUPS           list of supplementary groups of the new\n"
"                                account\n"
msgstr "  -G, --groups GROUPS\t新账户的附加组列表\n"

msgid "  -G, --groups GROUPS           new list of supplementary GROUPS\n"
msgstr "  -G, --groups GROUPS           新的附加组列表 GROUPS\n"

msgid ""
"  -I, --inactive INACTIVE       set password inactive after expiration\n"
"                                to INACTIVE\n"
msgstr "  -I, --inactive INACITVE       过期 INACTIVE 天数后，设定密码为失效状态\n"

msgid "  -K, --key KEY=VALUE           override /etc/login.defs defaults\n"
msgstr "  -K, --key KEY=VALUE           不使用 /etc/login.defs 中的默认值\n"

msgid "  -L, --lock                    lock the user account\n"
msgstr "  -L, --lock                    锁定用户帐号\n"

msgid ""
"  -M, --maxdays MAX_DAYS        set maximim number of days before password\n"
"                                change to MAX_DAYS\n"
msgstr "  -M, --maxdays 最大天数        将两次改变密码之间相距的最大天数设为“最大天数”\n"

msgid "  -M, --members USER,...        set the list of members of GROUP\n"
msgstr "  -M, --members USER,...        设置组 GROUP 的成员列表\n"

msgid ""
"  -M, --no-create-home          do not create the user's home directory\n"
msgstr "  -M, --no-create-home\t\t不创建用户的主目录\n"

msgid ""
"  -N, --no-user-group           do not create a group with the same name as\n"
"                                the user\n"
msgstr "  -N, --no-user-group\t不创建同名的组\n"

msgid "  -Q, --root CHROOT_DIR         directory to chroot into\n"
msgstr "  -Q, --root CHROOT_DIR         要 chroot 进的目录\n"

msgid ""
"  -R, --restrict                restrict access to GROUP to its members\n"
msgstr "  -R, --restrict                向其成员限制访问组 GROUP\n"

msgid "  -R, --root CHROOT_DIR         directory to chroot into\n"
msgstr "  -R, --root CHROOT_DIR         chroot 到的目录\n"

msgid ""
"  -S, --status                  report password status on the named "
"account\n"
msgstr "  -S, --status                  报告指定帐户密码的状态\n"

msgid "  -U, --unlock                  unlock the user account\n"
msgstr "  -U, --unlock                  解锁用户帐号\n"

msgid ""
"  -U, --user-group              create a group with the same name as the "
"user\n"
msgstr "  -U, --user-group\t\t创建与用户同名的组\n"

msgid ""
"  -W, --warndays WARN_DAYS      set expiration warning days to WARN_DAYS\n"
msgstr "  -W, --warndays 警告天数       将过期警告天数设为“警告天数”\n"

msgid ""
"  -Z, --selinux-user            remove any SELinux user mapping for the "
"user\n"
msgstr "  -Z, --selinux-user            为用户删除所有的 SELinux 用户映射\n"

msgid ""
"  -Z, --selinux-user SEUSER     new SELinux user mapping for the user "
"account\n"
msgstr "  -Z, --selinux-user  SEUSER       用户账户的新 SELinux 用户映射\n"

msgid ""
"  -Z, --selinux-user SEUSER     use a specific SEUSER for the SELinux user "
"mapping\n"
msgstr "  -Z, --selinux-user SEUSER\t\t为 SELinux 用户映射使用指定 SEUSER\n"

msgid "  -a, --add USER                add USER to GROUP\n"
msgstr "  -a, --add USER                向组 GROUP 中添加用户 USER\n"

msgid ""
"  -a, --add username            add username to the members of the group\n"
msgstr "  -a, --add username            将用户 username 添加到组成员中\n"

msgid "  -a, --all                     display faillog records for all users\n"
msgstr "  -a, --all\t\t\t为所有用户显示登陆失败记录\n"

msgid "  -a, --all                     report password status on all accounts\n"
msgstr "  -a, --all                     报告所有帐户的密码状态\n"

msgid ""
"  -a, --append                  append the user to the supplemental GROUPS\n"
"                                mentioned by the -G option without removing\n"
"                                him/her from other groups\n"
msgstr ""
"  -a, --append GROUP            将用户追加至上边 -G 中提到的附加组中，\n"
"                                并不从其它组中删除此用户\n"

msgid ""
"  -b, --base-dir BASE_DIR       base directory for the home directory of the\n"
"                                new account\n"
msgstr "  -b, --base-dir BASE_DIR\t新账户的主目录的基目录\n"

msgid ""
"  -b, --before DAYS             print only lastlog records older than DAYS\n"
msgstr "  -b, --before DAYS             仅打印早于 DAYS 的最近登录记录\n"

msgid "  -c, --check                   check the user's password expiration\n"
msgstr "  -c, --check                   检查用户密码是否过期\n"

msgid "  -c, --comment COMMENT         GECOS field of the new account\n"
msgstr "  -c, --comment COMMENT         新账户的 GECOS 字段\n"

msgid "  -c, --comment COMMENT         new value of the GECOS field\n"
msgstr "  -c, --comment 注释            GECOS 字段的新值\n"

msgid "  -c, --crypt-method METHOD     the crypt method (one of %s)\n"
msgstr "  -c, --crypt-method 方法        加密方法(%s 中的一个)\n"

msgid ""
"  -d, --delete                  delete the password for the named account\n"
msgstr "  -d, --delete                  删除指定帐户的密码\n"

msgid "  -d, --delete USER             remove USER from GROUP\n"
msgstr "  -d, --delete USER             从组 GROUP 中添加或删除用户\n"

msgid ""
"  -d, --delete username         remove username from the members of the "
"group\n"
msgstr "  -d, --delete username         从组的成员中删除用户 username\n"

msgid ""
"  -d, --home HOME_DIR           new home directory for the user account\n"
msgstr "  -d, --home HOME_DIR           用户的新主目录\n"

msgid "  -d, --home-dir HOME_DIR       home directory of the new account\n"
msgstr "  -d, --home-dir HOME_DIR       新账户的主目录\n"

msgid ""
"  -d, --lastday LAST_DAY        set date of last password change to "
"LAST_DAY\n"
msgstr "  -d, --lastday 最近日期        将最近一次密码设置时间设为“最近日期”\n"

msgid "  -e, --encrypted               supplied passwords are encrypted\n"
msgstr "  -e, --encrypted\t\t提供的密码已经加密\n"

msgid ""
"  -e, --expire                  force expire the password for the named "
"account\n"
msgstr "  -e, --expire                  强制使指定帐户的密码过期\n"

msgid "  -e, --expiredate EXPIRE_DATE  expiration date of the new account\n"
msgstr "  -e, --expiredate EXPIRE_DATE  新账户的过期日期\n"

msgid ""
"  -e, --expiredate EXPIRE_DATE  set account expiration date to EXPIRE_DATE\n"
msgstr "  -e, --expiredate EXPIRE_DATE  设定帐户过期的日期为 EXPIRE_DATE\n"

msgid ""
"  -f, --force                   exit successfully if the group already exists,\n"
"                                and cancel -g if the GID is already used\n"
msgstr ""
"  -f, --force\t\t如果组已经存在则成功退出\n"
"\t\t\t并且如果 GID 已经存在则取消 -g\n"

msgid ""
"  -f, --force                   force password change if the user's password\n"
"                                is expired\n"
msgstr "  -f, --force                   如果用户密码过期，则强制要求更改密码\n"

msgid ""
"  -f, --force                   force removal of files,\n"
"                                even if not owned by user\n"
msgstr "  -f, --force                   即使不属于此用户，也强制删除文件\n"

msgid "  -f, --full-name FULL_NAME     change user's full name\n"
msgstr "  -f, --full-name FULL_NAME     更改用户的全名\n"

msgid ""
"  -f, --inactive INACTIVE       password inactivity period of the new "
"account\n"
msgstr "  -f, --inactive INACTIVE       新账户的密码不活动期\n"

msgid ""
"  -f, --inactive INACTIVE       set password inactive after expiration\n"
"                                to INACTIVE\n"
msgstr "  -f, --inactive INACTIVE       过期 INACTIVE 天数后，设定密码为失效状态\n"

msgid "  -g, --gid GID                 change the group ID to GID\n"
msgstr "  -g, --gid GID                 将组 ID 改为 GID\n"

msgid "  -g, --gid GID                 use GID for the new group\n"
msgstr "  -g, --gid GID                 为新组使用 GID\n"

msgid "  -g, --gid GROUP               force use GROUP as new primary group\n"
msgstr "  -g, --gid GROUP               强制使用 GROUP 为新主组\n"

msgid ""
"  -g, --gid GROUP               name or ID of the primary group of the new\n"
"                                account\n"
msgstr "  -g, --gid GROUP\t\t新账户主组的名称或 ID\n"

msgid "  -g, --group                   edit group database\n"
msgstr "  -g, --group                   编辑 group 数据库\n"

msgid ""
"  -g, --group groupname         change groupname instead of the user's group\n"
"                                (root only)\n"
msgstr "  -g, --group groupname         更改组 groupname，而不是用户的组(只 root)\n"

msgid "  -h, --help                    display this help message and exit\n"
msgstr "  -h, --help                    显示此帮助信息并推出\n"

msgid "  -h, --home-phone HOME_PHONE   change user's home phone number\n"
msgstr "  -h, --home-phone HOME_PHONE   更改用户的家庭电话号码\n"

msgid ""
"  -i, --inactive INACTIVE       set password inactive after expiration\n"
"                                to INACTIVE\n"
msgstr "  -i, --inactive INACTIVE       密码过期后设置密码不活动为 INACTIVE\n"

msgid "  -k, --keep-tokens             change password only if expired\n"
msgstr "  -k, --keep-tokens             仅在过期后修改密码\n"

msgid ""
"  -k, --skel SKEL_DIR           use this alternative skeleton directory\n"
msgstr "  -k, --skel SKEL_DIR\t使用此目录作为骨架目录\n"

msgid "  -l, --list                    list the members of the group\n"
msgstr "  -l, --list                    列出组中的所有成员\n"

msgid "  -l, --list                    show account aging information\n"
msgstr "  -l, --list                    显示帐户年龄信息\n"

msgid "  -l, --lock                    lock the password of the named account\n"
msgstr "  -l, --lock                    锁定指定的帐户\n"

msgid ""
"  -l, --lock-secs SEC           after failed login lock account for SEC "
"seconds\n"
msgstr "  -l, --lock-secs SEC\t登陆失败后临时锁定账户 %s 秒\n"

msgid "  -l, --login NEW_LOGIN         new value of the login name\n"
msgstr "  -l, --login LOGIN             新的登录名称\n"

msgid ""
"  -l, --no-log-init             do not add the user to the lastlog and\n"
"                                faillog databases\n"
msgstr "  -l, --no-log-init\t不要将此用户添加到最近登录和登录失败数据库\n"

msgid "  -m, --create-home             create the user's home directory\n"
msgstr "  -m, --create-home\t创建用户的主目录\n"

msgid ""
"  -m, --maximum MAX             set maximum failed login counters to MAX\n"
msgstr "  -m, --maximum MAX\t\t将最大登陆失败计数设置为 MAX\n"

msgid ""
"  -m, --md5                     encrypt the clear text password using\n"
"                                the MD5 algorithm\n"
msgstr "  -m, --md5\t\t使用 MD5 算法加密明文密码\n"

msgid ""
"  -m, --mindays MIN_DAYS        set minimum number of days before password\n"
"                                change to MIN_DAYS\n"
msgstr "  -m, --mindays 最小天数        将两次改变密码之间相距的最小天数设为“最小天数”\n"

msgid ""
"  -m, --move-home               move contents of the home directory to the\n"
"                                new location (use only with -d)\n"
msgstr "  -m, --move-home               将家目录内容移至新位置 (仅于 -d 一起使用)\n"

msgid ""
"  -n, --mindays MIN_DAYS        set minimum number of days before password\n"
"                                change to MIN_DAYS\n"
msgstr ""
"  -n, --mindays MIN_DAYS        设置到下次修改密码所须等待的最短天数\n"
"                                为 MIN_DAYS\n"

msgid "  -n, --new-name NEW_GROUP      change the name to NEW_GROUP\n"
msgstr "  -n, --new-name NEW_GROUP      改名为 NEW_GROUP\n"

msgid ""
"  -o, --non-unique              allow to create groups with duplicate\n"
"                                (non-unique) GID\n"
msgstr "  -o, --non-unique              允许创建有重复 GID 的组\n"

msgid ""
"  -o, --non-unique              allow to create users with duplicate\n"
"                                (non-unique) UID\n"
msgstr "  -o, --non-unique\t\t允许使用重复的 UID 创建用户\n"

msgid ""
"  -o, --non-unique              allow to use a duplicate (non-unique) GID\n"
msgstr "  -o, --non-unique              允许使用重复的 GID\n"

msgid "  -o, --non-unique              allow using duplicate (non-unique) UID\n"
msgstr "  -o, --non-unique              允许使用重复的(非唯一的) UID\n"

msgid "  -o, --other OTHER_INFO        change user's other GECOS information\n"
msgstr "  -o, --other OTHER_INFO        更改用户的其它 GECOS 信息\n"

msgid "  -p, --passwd                  edit passwd database\n"
msgstr "  -p, --passwd                  编辑 passwd 数据库\n"

msgid ""
"  -p, --password PASSWORD       change the password to this (encrypted)\n"
"                                PASSWORD\n"
msgstr "  -p, --password PASSWORD\t将密码更改为(加密过的) PASSWORD\n"

msgid "  -p, --password PASSWORD       encrypted password of the new account\n"
msgstr "  -p, --password PASSWORD\t\t加密后的新账户密码\n"

msgid ""
"  -p, --password PASSWORD       use encrypted password for the new "
"password\n"
msgstr "  -p, --password PASSWORD       将加密过的密码 (PASSWORD) 设为新密码\n"

msgid ""
"  -p, --password PASSWORD       use this encrypted password for the new "
"group\n"
msgstr "  -p, --password PASSWORD       为新组使用此加密过的密码\n"

msgid "  -p, --purge                   purge all members from the group\n"
msgstr "  -p, --purge                   从组中移除所有成员\n"

msgid "  -q, --quiet                   quiet mode\n"
msgstr "  -q, --quiet                   安静模式\n"

msgid "  -q, --quiet                   report errors only\n"
msgstr "  -q, --quiet                   只报告错误\n"

msgid ""
"  -r, --read-only               display errors and warnings\n"
"                                but do not change files\n"
msgstr "  -r, --read-only               显示错误和警告，但不改变文件\n"

msgid "  -r, --remove                  remove home directory and mail spool\n"
msgstr "  -r, --remove                  删除主目录和邮件池\n"

msgid "  -r, --remove-password         remove the GROUP's password\n"
msgstr "  -r, --remove-password         移除组 GROUP 的密码\n"

msgid ""
"  -r, --repository REPOSITORY   change password in REPOSITORY repository\n"
msgstr "  -r, --repository REPOSITORY   在 REPOSITORY 库中改变密码\n"

msgid "  -r, --reset                   reset the counters of login failures\n"
msgstr "  -r, --reset\t\t重置登陆失败计数\n"

msgid "  -r, --room ROOM_NUMBER        change user's room number\n"
msgstr "  -r, --room ROOM_NUMBER        更改用户的房间号\n"

msgid "  -r, --system                  create a system account\n"
msgstr "  -r, --system                  创建一个系统账户\n"

msgid "  -r, --system                  create system accounts\n"
msgstr "  -r, --system                  创建系统帐号\n"

msgid ""
"  -s, --sha-rounds              number of SHA rounds for the SHA*\n"
"                                crypt algorithms\n"
msgstr "  -s, --sha-rounds\t\tSHA* 加密算法中的 SHA 旁边的数字\n"

msgid "  -s, --shadow                  edit shadow or gshadow database\n"
msgstr "  -s, --shadow                  编辑 shadow 或 gshadow 数据库\n"

msgid "  -s, --shell SHELL             login shell of the new account\n"
msgstr "  -s, --shell SHELL\t\t新账户的登录 shell\n"

msgid "  -s, --shell SHELL             new login shell for the user account\n"
msgstr "  -s, --shell SHELL             该用户帐号的新登录 shell\n"

msgid "  -s, --sort                    sort entries by UID\n"
msgstr "  -s, --sort                    通过 UID 排序项目\n"

msgid ""
"  -t, --time DAYS               display faillog records more recent than "
"DAYS\n"
msgstr "  -t, --time DAYS\t\t显示 DAYS 天来的登陆失败记录\n"

msgid ""
"  -t, --time DAYS               print only lastlog records more recent than "
"DAYS\n"
msgstr "  -t, --time DAYS               仅打印晚于 DAYS 的最近登录记录\n"

msgid "  -u, --help                    display this help message and exit\n"
msgstr "  -u, --help                    显示此帮助信息并推出\n"

msgid "  -u, --uid UID                 new UID for the user account\n"
msgstr "  -u, --uid UID                 用户帐号的新 UID\n"

msgid "  -u, --uid UID                 user ID of the new account\n"
msgstr "  -u, --uid UID\t\t\t新账户的用户 ID\n"

msgid ""
"  -u, --unlock                  unlock the password of the named account\n"
msgstr "  -u, --unlock                  解锁被指定帐户\n"

msgid "  -u, --user                    which user's tcb shadow file to edit\n"
msgstr "  -u, --user                    要编辑哪个用户的 tcb 影子文件\n"

msgid ""
"  -u, --user LOGIN              print lastlog record of the specified "
"LOGIN\n"
msgstr "  -u, --user LOGIN              打印 LOGIN 用户的最近登录记录\n"

msgid ""
"  -u, --user LOGIN/RANGE        display faillog record or maintains failure\n"
"                                counters and limits (if used with -r, -m,\n"
"                                or -l) only for the specified LOGIN(s)\n"
msgstr ""
"  -u, --user LOGIN/RANGE\t只为指定的登录显示登录失败记录或维护失败\n"
"\t\t\t\t\t计数和限制(如果使用了 -r, -m 或 -l)\n"

msgid ""
"  -w, --warndays WARN_DAYS      set expiration warning days to WARN_DAYS\n"
msgstr "  -w, --warndays WARN_DAYS      设置过期警告天数为 WARN_DAYS\n"

msgid "  -w, --work-phone WORK_PHONE   change user's office phone number\n"
msgstr "  -w, --work-phone WORK_PHONE   更改用户的办公室电话号码\n"

msgid ""
"  -x, --maxdays MAX_DAYS        set maximum number of days before password\n"
"                                change to MAX_DAYS\n"
msgstr ""
"  -x, --maxdays MAX_DAYS        设置到下次修改密码所须等待的最多天数\n"
"                                为 MAX_DAYS\n"

msgid "  Choose a new password."
msgstr "  请选择一个新密码。"

msgid "  Contact the system administrator."
msgstr "  请与系统管理员联系。"

msgid " ...killed.\n"
msgstr " ...已被杀死。\n"

msgid " ...terminated.\n"
msgstr " ...已结束。\n"

msgid " ...waiting for child to terminate.\n"
msgstr " ...等待子进程结束。\n"

msgid " [%lds lock]"
msgstr " [锁定 %lds]"

msgid " [%lus left]"
msgstr " [还剩 %lus]"

msgid " from %.*s"
msgstr " 来自 %.*s"

msgid " groups="
msgstr " 组="

msgid ""
"%d failure since last login.\n"
"Last was %s on %s.\n"
msgid_plural ""
"%d failures since last login.\n"
"Last was %s on %s.\n"
msgstr[0] ""
"自上一次登录以来已有 %d 次登录失败。\n"
"最后一次是 %s 在 %s 上。\n"

msgid "%s login: "
msgstr "%s 用户名： "

msgid "%s's Password: "
msgstr "%s 的密码： "

msgid "%s: "
msgstr "%s： "

msgid "%s: %s\n"
msgstr "%s：%s\n"

msgid ""
"%s: %s\n"
"(Ignored)\n"
msgstr ""
"%s：%s\n"
"(忽略)\n"

msgid "%s: %s flag is only allowed with the %s flag\n"
msgstr "%s：%s 标记只能和 %s 标记一起使用\n"

msgid "%s: %s home directory (%s) not found\n"
msgstr "%s：未找到 %s 的主目录“%s”\n"

msgid "%s: %s is an invalid shell\n"
